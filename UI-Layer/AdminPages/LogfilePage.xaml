<Page x:Class="TMS_ap_dg_js_sm.LogfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TMS_ap_dg_js_sm"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="650"
      Title="LogfilePage"
      Background="White">

    <Grid >
        <ListBox x:Name="LogfileDisplay" Margin="55,61,56,58"/>
        <TextBox x:Name="filePathBox" HorizontalAlignment="Left" Height="20" Margin="283,36,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="231" IsEnabled="False"/>
        <Button x:Name="OpenBtn" Content="Open" HorizontalAlignment="Left" Margin="519,36,0,0" VerticalAlignment="Top" Width="75" Click="OpenBtn_Click"/>
    </Grid>
</Page>
